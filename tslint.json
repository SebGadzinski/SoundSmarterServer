{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "interface-name": [false],
    "quotemark": [true, "single"],
    "no-console": false,
    "curly": [true, "ignore-same-line"],
    "object-literal-sort-keys": false,
    "trailing-comma": [
      false,
      {
        "singleline": "never",
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ]
  },
  "rulesDirectory": []
}
